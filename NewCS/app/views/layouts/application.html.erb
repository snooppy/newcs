
<html>
  <head>
    <title>NewCS</title>
    <%= stylesheet_link_tag    "application" %>
    <%= javascript_include_tag 'jquery', 'application' %>
    <%#=# javascript_include_tag 'jquery-1.6.2.min', 'jquery-ui-1.8.16.custom.min' %>
    <%= csrf_meta_tags %>
    <%if !session[:user].nil?%>
      <script type="text/javascript" >
        $(document).ready(
        function(){
          event_scan();
          setInterval(function (){
            event_scan();
          }, 1000000);
        
          //$("#new_message").draggable();
        });
      </script>
    <%end%>
  </head>
  <body>
    <%=render :partial=>"messages/form"%>
    <%=render :partial=>"users/auth"%>
    <%=render :partial=>"layouts/right_menu"%>

    <div id ="notification_area">

    </div>
    <div id ="popup_message">

    </div>

    <div class="container">

      <section>
        <%= yield %>
      </section>

    </div>
  </body>
</html>
