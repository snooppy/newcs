<%= form_for(@shedule) do |f| %>
  <div class="field">
    <%= f.label :week %><br />
    <%= f.number_field :week %>
  </div>
  <div class="field">
    <%= f.label :day %><br />
    <%= f.number_field :day %>
  </div>
  <div class="field">
    <%= f.label :hour %><br />
    <%= f.number_field :hour %>
  </div>
  <div class="field">
    <%= f.label :classroom %><br />
    <%= f.number_field :classroom %>
  </div>
  <div class="field">
    <%= f.label :sub_group2 %><br />
    <%= f.number_field :sub_group2 %>
  </div>
  <div class="field">
    <%= f.label :sub_group3 %><br />
    <%= f.number_field :sub_group3 %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
