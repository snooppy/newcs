<div id="title">Мои настройки</div>

<table>
  <tr>
    <th>Images</th>
    <th>Students</th>
    <th>Subjects</th>
    <th>Shedules</th>
    <th>Marks</th>
    <th>Files</th>
    <th>Publications</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @settings.each do |setting| %>
  <tr>
    <td><%= setting.images %></td>
    <td><%= setting.students %></td>
    <td><%= setting.subjects %></td>
    <td><%= setting.shedules %></td>
    <td><%= setting.marks %></td>
    <td><%= setting.files %></td>
    <td><%= setting.publications %></td>
    <td><%= link_to 'Show', setting %></td>
    <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
    <td><%= link_to 'Destroy', setting, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Setting', new_setting_path %>
