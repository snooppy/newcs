<%= form_for(@user, :remote => true,:url => "/users/create") do |f| %>
  <div class="field">
    <%= f.label :login %><br />
    <%= f.text_field :login %><br>
    <%= f.label :password %><br />
    <%= f.text_field :password %><br> # TODO add match passwords
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
